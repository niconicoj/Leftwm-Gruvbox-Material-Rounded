{% for tag in workspace.tags %}
{% if tag.mine %}
%{A1:$SCRIPTPATH/change_to_tag {{workspace.index}} {{tag.index}}:}
%{T1}%{F#d4be98}{{tag.name}}%{F#89b482}· %{F-}
%{A}
{% elsif tag.visible  %}
%{A1:$SCRIPTPATH/change_to_tag {{workspace.index}} {{tag.index}}:}
%{T1}%{F#d4be98}{{tag.name}}%{F#bd93f9}· %{F-}
%{A}
{% elsif tag.busy  %}
%{A1:$SCRIPTPATH/change_to_tag {{workspace.index}} {{tag.index}}:}
%{T1}%{F#d4be98}{{tag.name}}· %{F-}
%{A}
{% else tag.visible  %}
%{A1:$SCRIPTPATH/change_to_tag {{workspace.index}} {{tag.index}}:}
%{F#d4be98}%{T1}{{tag.name}}  %{F-}
%{A}
{% endif %}
{% endfor %}
